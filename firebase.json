{
  "functions": {
    "source": "functions"
  },
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/kupu",
        "function": "kupu"
      } , {
        "source": "**",
        "destination": "/index.html"
      }]
  }
}
